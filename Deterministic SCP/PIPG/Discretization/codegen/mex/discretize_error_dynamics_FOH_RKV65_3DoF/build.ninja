# CompilerName=msvc170
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root\build\win64
matlabdir = C:\Program Files\MATLAB\R2024b
startdir = C:\Users\thatf\OneDrive\Documents\Purdue Classes\AAE 590ACA\AAE590ACA-Stochastic-SCP-Rocket-Landing\Deterministic SCP\PIPG\Discretization

# Toolchain information
cc = cl
cxx = cl
linker = link
linkerxx = link

cflags =  /c /Zc:__cplusplus /Zp8 /GR /W3 /EHs /nologo /MD /D_CRT_SECURE_NO_DEPRECATE /D_SCL_SECURE_NO_DEPRECATE /D_SECURE_SCL=0   /DMATLAB_MEX_FILE  /DMATLAB_MEX_FILE /O2 /Oy- /DNDEBUG /fp:strict /utf-8 /I "$root" /I "$startdir" /I "$root\interface" /I "$matlabdir\extern\include" /I "." -DMODEL=discretize_error_dynamics_FOH_RKV65_3DoF_mex  

cxxflags =  /c /Zc:__cplusplus /Zp8 /GR /W3 /EHs /nologo /MD /D_CRT_SECURE_NO_DEPRECATE /D_SCL_SECURE_NO_DEPRECATE /D_SECURE_SCL=0   /DMATLAB_MEX_FILE  /DMATLAB_MEX_FILE /O2 /Oy- /DNDEBUG /fp:strict /utf-8 /I "$root" /I "$startdir" /I "$root\interface" /I "$matlabdir\extern\include" /I "." -DMODEL=discretize_error_dynamics_FOH_RKV65_3DoF_mex  

ldflags =  /nologo    /DLL /LIBPATH:"$matlabdir\extern\lib\win64\microsoft" libmx.lib libmex.lib libmat.lib kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib  /out:"discretize_error_dynamics_FOH_RKV65_3DoF_mex.mexw64" libemlrt.lib libcovrt.lib libut.lib libmwmathutil.lib  /export:mexFunction /export:mexfilerequiredapiversion /export:emlrtMexFcnProperties /export:discretize_error_dynamics_FOH_RKV65_3DoF /export:discretize_error_dynamics_FOH_RKV65_3DoF_initialize /export:discretize_error_dynamics_FOH_RKV65_3DoF_terminate /export:discretize_error_dynamics_FOH_RKV65_3DoF_atexit /export:emxEnsureCapacity_real_T /export:emxFree_real_T /export:emxInit_real_T

ldxxflags =  /nologo    /DLL /LIBPATH:"$matlabdir\extern\lib\win64\microsoft" libmx.lib libmex.lib libmat.lib kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib libMatlabDataArray.lib libMatlabEngine.lib  /out:"discretize_error_dynamics_FOH_RKV65_3DoF_mex.mexw64" libemlrt.lib libcovrt.lib libut.lib libmwmathutil.lib  /export:mexFunction /export:mexfilerequiredapiversion /export:emlrtMexFcnProperties /export:discretize_error_dynamics_FOH_RKV65_3DoF /export:discretize_error_dynamics_FOH_RKV65_3DoF_initialize /export:discretize_error_dynamics_FOH_RKV65_3DoF_terminate /export:discretize_error_dynamics_FOH_RKV65_3DoF_atexit /export:emxEnsureCapacity_real_T /export:emxFree_real_T /export:emxInit_real_T

linkobjs = 

# Build Rules
rule cc
    command = $cc $cflags $in /Fo$out

rule cxx
    command = $cxx $cxxflags $in /Fo$out

rule link
    command = $linker @$out.rsp $linkobjs $ldflags
    rspfile = $out.rsp
    rspfile_content = $in

rule linkxx
    command = $linkerxx $in $linkobjs $ldxxflags

# Build
build $builddir\discretize_error_dynamics_FOH_RKV65_3DoF_data.obj : cc $root\discretize_error_dynamics_FOH_RKV65_3DoF_data.c
build $builddir\rt_nonfinite.obj : cc $root\rt_nonfinite.c
build $builddir\discretize_error_dynamics_FOH_RKV65_3DoF_initialize.obj : cc $root\discretize_error_dynamics_FOH_RKV65_3DoF_initialize.c
build $builddir\discretize_error_dynamics_FOH_RKV65_3DoF_terminate.obj : cc $root\discretize_error_dynamics_FOH_RKV65_3DoF_terminate.c
build $builddir\discretize_error_dynamics_FOH_RKV65_3DoF.obj : cc $root\discretize_error_dynamics_FOH_RKV65_3DoF.c
build $builddir\linspace.obj : cc $root\linspace.c
build $builddir\eye.obj : cc $root\eye.c
build $builddir\pagemtimes.obj : cc $root\pagemtimes.c
build $builddir\reshapeSizeChecks.obj : cc $root\reshapeSizeChecks.c
build $builddir\zero_if_empty.obj : cc $root\zero_if_empty.c
build $builddir\vecnorm.obj : cc $root\vecnorm.c
build $builddir\_coder_discretize_error_dynamics_FOH_RKV65_3DoF_api.obj : cc $root\interface\_coder_discretize_error_dynamics_FOH_RKV65_3DoF_api.c
build $builddir\_coder_discretize_error_dynamics_FOH_RKV65_3DoF_mex.obj : cc $root\interface\_coder_discretize_error_dynamics_FOH_RKV65_3DoF_mex.c
build $builddir\eml_int_forloop_overflow_check.obj : cc $root\eml_int_forloop_overflow_check.c
build $builddir\discretize_error_dynamics_FOH_RKV65_3DoF_emxutil.obj : cc $root\discretize_error_dynamics_FOH_RKV65_3DoF_emxutil.c
build $builddir\_coder_discretize_error_dynamics_FOH_RKV65_3DoF_info.obj : cc $root\interface\_coder_discretize_error_dynamics_FOH_RKV65_3DoF_info.c
build $builddir\c_mexapi_version.obj : cc $matlabdir\extern\version\c_mexapi_version.c

# Link
build $root\discretize_error_dynamics_FOH_RKV65_3DoF_mex.mexw64 : link $builddir\discretize_error_dynamics_FOH_RKV65_3DoF_data.obj $builddir\rt_nonfinite.obj $builddir\discretize_error_dynamics_FOH_RKV65_3DoF_initialize.obj $builddir\discretize_error_dynamics_FOH_RKV65_3DoF_terminate.obj $builddir\discretize_error_dynamics_FOH_RKV65_3DoF.obj $builddir\linspace.obj $builddir\eye.obj $builddir\pagemtimes.obj $builddir\reshapeSizeChecks.obj $builddir\zero_if_empty.obj $builddir\vecnorm.obj $builddir\_coder_discretize_error_dynamics_FOH_RKV65_3DoF_api.obj $builddir\_coder_discretize_error_dynamics_FOH_RKV65_3DoF_mex.obj $builddir\eml_int_forloop_overflow_check.obj $builddir\discretize_error_dynamics_FOH_RKV65_3DoF_emxutil.obj $builddir\_coder_discretize_error_dynamics_FOH_RKV65_3DoF_info.obj $builddir\c_mexapi_version.obj 
