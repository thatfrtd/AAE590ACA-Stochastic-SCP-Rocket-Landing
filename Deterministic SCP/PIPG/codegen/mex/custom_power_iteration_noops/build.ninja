cxxflags =  -c -fexceptions -fno-omit-frame-pointer -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c++11 -I "C:\Users\thatf\OneDrive\Documents\Purdue Classes\AAE 590ACA\AAE590ACA-Stochastic-SCP-Rocket-Landing\Deterministic SCP\PIPG\codegen\mex\custom_power_iteration_noops" -I "C:\Users\thatf\OneDrive\Documents\Purdue Classes\AAE 590ACA\AAE590ACA-Stochastic-SCP-Rocket-Landing\Deterministic SCP\PIPG" -I "C:\Users\thatf\OneDrive\Documents\Purdue Classes\AAE 590ACA\AAE590ACA-Stochastic-SCP-Rocket-Landing\Deterministic SCP\PIPG\codegen\mex\custom_power_iteration_noops\interface" -I "C:\Program Files\MATLAB\R2024b\extern\include" -I "." -DMODEL=custom_power_iteration_noops_mex -D__USE_MINGW_ANSI_STDIO=1 -Wno-stringop-overflow -Wno-maybe-uninitialized 

