build $builddir/vecnorm.o : cc $root/vecnorm.c
build $builddir/sumMatrixIncludeNaN.o : cc $root/sumMatrixIncludeNaN.c
build $builddir/_coder_custom_power_iteration_noops_api.o : cc $root/interface/_coder_custom_power_iteration_noops_api.c
